# mount point   # fstype   # device                                       # flags
/boot           emmc       /dev/block/bootdevice/by-name/boot  
/system_root    ext4       /dev/block/bootdevice/by-name/system           flags=display="System"
/system_image   emmc       /dev/block/bootdevice/by-name/system            
/vendor         ext4       /dev/block/bootdevice/by-name/vendor           flags=backup=1;wipeingui
/vendor_image   emmc       /dev/block/bootdevice/by-name/vendor           flags=backup=1
/data           ext4       /dev/block/bootdevice/by-name/userdata         flags=encryptable=footer
/cache          ext4       /dev/block/bootdevice/by-name/cache            
/misc		    emmc	   /dev/block/bootdevice/by-name/misc
/recovery       emmc       /dev/block/bootdevice/by-name/recovery         flags=backup=1
/efs1		    emmc	   /dev/block/bootdevice/by-name/modemst1		  flags=backup=1;display=EFS
/efs2		    emmc	   /dev/block/bootdevice/by-name/modemst2	      flags=backup=1;subpartitionof=/efs1
/efsg           emmc	   /dev/block/bootdevice/by-name/fsg              flags=backup=1;subpartitionof=/efs1
/efsc           emmc	   /dev/block/bootdevice/by-name/fsc              flags=backup=1;subpartitionof=/efs1

# Removable storage
# mount point   # fstype   # device                 # logical fs          # flags
/external_sd	vfat       /dev/block/mmcblk1p1		/dev/block/mmcblk1    flags=fsflags=utf8;display="External SDcard";storage;wipeingui;removable
/usb_otg        vfat       /dev/block/sda1          /dev/block/sda        flags=fsflags=utf8;display="USB-OTG";storage;wipeingui;removable

# Experimental
/opporeserve            ext4    /dev/block/bootdevice/by-name/opporeserve2
/mnt/vendor/persist     ext4    /dev/block/bootdevice/by-name/persist
/vendor/bt_firmware     vfat    /dev/block/bootdevice/by-name/bluetooth
/vendor/dsp             ext4    /dev/block/bootdevice/by-name/dsp
/vendor/firmware_mnt    vfat    /dev/block/bootdevice/by-name/modem
/storage/sdcard1        vfat    /devices/platform/soc/8804000.sdhci/mmc_host*
/storage/sdcard1        vfat    /devices/platform/soc/1da4000.ufshc_card/host*
